    "devDependencies": {
        "npm-run-all": "^4.1.5",
        "parcel-bundler": "^1.12.4",
        "sass": "^1.32.12",
        "typescript": "^4.2.4"
    },
    "dependencies": {
        "bootstrap": "^4.6.0",
        "bootstrap-icons": "^1.4.1",
        "jquery": "^3.6.0",
        "popper.js": "^1.16.1"
    }
}
